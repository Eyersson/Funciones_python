print("---------------------------------")
print("mostar cadena N veses en pantalla")
print("---------------------------------")

def mostrarCadena(cadena, n):
    for i in range(n):
        print(f"{i} . {cadena}")

#entrada
cadena = input("Digite la cadena a mostar: ")
n = int(input("Digite el numero de ves que quiere mostrar la cadena: "))

#procesamiento
mostrarCadena(cadena, n)

#salida
print("\nEso era...")